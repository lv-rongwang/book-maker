<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.njucm.book.chart.dao.TabContentMapper">
    <insert id="addTabContent">

        insert into tab_content values(#{id},#{content})

    </insert>

    <select id="findById" resultType="tabContent">

        select * from tab_content where id=#{id}

    </select>
</mapper>